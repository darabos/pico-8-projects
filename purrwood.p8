function _update()
 t+=1
 if mode=="puzzle" then
  updatepuzzle()
 elseif mode=="startpuzzle" then
  if puzzle.start<t-20 then
   mode="puzzle"
  end
 else
  updatesea()
 end
end

function updatepuzzle()
 if #puzzle.board==2 then
  if puzzle.finished==nil then
   puzzle.finished=t
  elseif puzzle.finished+50<t then
   mode="sea"
   jelly.y-=200
  end
  return
 end
 local px=0
 local py=0
 if (btnp(‚¨ÖÔ∏è)) px-=1
 if (btnp(‚û°Ô∏è)) px+=1
 if (btnp(‚¨ÜÔ∏è)) py-=1
 if (btnp(‚¨áÔ∏è)) py+=1
 if (px==0 and py==0) return
 sfx(13,-1,0,4)
 local b={}
 foreach(puzzle.board, function(j)
  b[j.x+10*j.y]=j
 end)
 foreach(puzzle.board, function(j)
  if (j.pillar) return
  local x=j.x
  local y=j.y
  for i=1,7 do
   x+=px
   y+=py
   if (x<0 or x>6) return
   if (y<0 or y>3 and x!=3) return
   if b[x+10*y]==nil then
    j.x+=px
    j.y+=py
    j.ox-=16*px
    j.oy-=16*py
    if j.y>3 then
     j.y+=4.5
     j.oy-=56
     add(puzzle.jumpers,j)
     del(puzzle.board,j)
    end
    return
   elseif b[x+10*y].pillar then
    return
   end
  end
 end)
end

