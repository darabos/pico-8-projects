islands={}
function addisland(x,y,w,h)
 local i={
  x=x,y=y,w=w,h=h,
drawwaves=function(self)
 for sx=0,self.w do for sy=0,self.h do
  cx=self.x+6*sx
  cy=self.y+6*sy
  f=11+sin((t/12+cy/20)%6.9/6.9)*3
  oval(cx-f+0.5,cy+6-f/2,
  cx+f+0.5,cy+6+f/2,6)
 end end
end,
draw=function(self)
 for sx=0,self.w do for sy=0,self.h do
  circfill(self.x+sx*6,self.y+sy*6,8,11)
 end end
 for sx=0,self.w do for sy=0,self.h do
  circfill(self.x+sx*6,self.y+2+sy*6,7,4)
 end end
 for sx=0,self.w do for sy=0,self.h do
  circfill(self.x+sx*6,self.y+sy*6,7,3)
 end end
end
}
 add(islands,i)
 for sx=0,w do for sy=0,h do
  if rnd()<0.1 then
   add(gems,{x=x+sx*6,y=y+sy*6+1,c=flr(rnd()*4),island=i})
  end
 end end
 return i
end
