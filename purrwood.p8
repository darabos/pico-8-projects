  foreach(islands, function(i)
   if boatcollide(boat,
    i.x-14,i.y-10,
    i.x+i.w*6+15,i.y+i.h*6+15) then
    kitten.in_boat=false
    kitten.on_island=i
    kitten.disembarked=false
    if i==shopisland then
     shoptext="meow to buy something"
     if (gold==0) shoptext="welcome to my shop"
    end
    boat.vx=0
    boat.vy=0
  end
  end)
  local jx=boat.x-jelly.x
  local jy=boat.y-jelly.y
  debug=jx*jx+jy*jy
  -- avoid overflow
  if abs(jx)+abs(jy)<100 and
   jx*jx+jy*jy<200 then
   startpuzzle()
  end
 else -- on land
  
 end
end

function boatcollide(boat,x1,y1,x2,y2)
 local bx=boat.x
 local by=boat.y
 if x1<bx and bx<x2
 and y1<by and by<y2 then
  if bx-x1 < x2-bx then
   if bx-x1 < by-y1 then
    if bx-x1 < y2-by then
     bx=x1
    else
     by=y2
    end
   else
    if by-y1 < y2-by then
     by=y1
    else
     by=y2
    end
   end
  else
   if x2-bx < by-y1 then
    if x2-bx < y2-by then
     bx=x2
    else
     by=y2
    end
   else
    if by-y1 < y2-by then
     by=y1
    else
     by=y2
    end
   end
  end
 end
 if bx!=boat.x then
  boat.x=bx
  return true
 elseif by!=boat.y then
  boat.y=by
  return true
 end
 return false
end

function surfacewaves()
 for x=-10,10 do for y=-10,10 do
  local lx=(cam.x\10+x)*10
  local ly=(cam.y\10+y)*10
  local f=sin(0.01*t+0.11*ly)*10
  lx+=f/3
  f+=t+lx+0.3*ly
  ly+=sin(0.02*f)*3
  local w=-1+sin(0.01*f)*3
  if (w>0) line(lx,ly,lx+w,ly,6)
 end end
end

function movejelly()
 if kitten.in_boat and abs(boat.vx)+abs(boat.vy)>0 then
  if (boat.x>jelly.x) jelly.vx+=0.1
  if (boat.x<jelly.x) jelly.vx-=0.1
  if (boat.y>jelly.y) jelly.vy+=0.1
  if (boat.y<jelly.y) jelly.vy-=0.1
 end
 jelly.vx=mid(-1,jelly.vx,1)
 jelly.vy=mid(-1,jelly.vy,1)
 jelly.x+=jelly.vx
 jelly.y+=jelly.vy
end

function movekitten()
 if (kitten.in_boat) return
 local px=0
 local py=0
 if (btn(‚¨ÖÔ∏è)) px-=1
 if (btn(‚û°Ô∏è)) px+=1
 if (btn(‚¨ÜÔ∏è)) py-=1
 if (btn(‚¨áÔ∏è)) py+=1
 if (px>0) kitten.flip=true
 if (px<0) kitten.flip=false
 kitten.x+=px
 kitten.y+=py
 foreach(gems,function(g)
  if (g.island!=kitten.on_island) return
  local dx=kitten.x-g.x
  local dy=kitten.y-g.y-5
  if dx*dx+dy*dy<40 then
   gold+=g.c+1
   del(gems,g)
   sfx(11,-1,g.c*4,4)
  end
 end)
 boardingcheck()
end

function boardingcheck()
 local dx=kitten.x-boat.x
 local dy=kitten.y-boat.y-5
 if dx*dx+dy*dy<40 then
  if kitten.disembarked then
   kitten.in_boat=true
  end
 else
  kitten.disembarked=true
 end
end
