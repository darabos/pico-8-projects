--from ultrabrite
function sortobjects()
 local t=objects
 for i=2,#t do
  local j=i
  while j>1 and
   t[j].y<t[j-1].y do
   t[j],t[j-1]=t[j-1],t[j]
   j-=1
  end
 end
end

function _update()
 foreach(objects,function(o)
  if o.update!=nil then
   o.update(o) end
 end)
 --sync to music
 f=(f+1)%3000
 local tick=stat(56)
 if tick<100 and last_tick>100 then
  f=(f+18)\36*36
 end
 last_tick=tick
 --transition
 if transition!=nil
  and transition.switch!=nil
  and t()>transition.start+0.8 then
  transition.switch()
  transition.switch=nil
 elseif transition!=nil
  and transition.switch==nil
  and t()>transition.start+1.6 then
  transition=nil
 end
 --metaverse
 if location=="metaverse" then
  if score and score<minscore then
   p.alert=nil
  else
   p.alert=daniel
  end
 end
end

