 for k=1, #wrds do
  wrd=strspl(wrds[k],"\n")
  for n=1,#wrd do
   if len+#wrd[n]>w or n>1 then
    ret=ret.."\n"
    len=0
   end
   ret=ret..wrd[n].." "
   len+=#wrd[n]+1
  end
 end
 if sub(ret,#ret-1,#ret-1)=="‚ùé" then
  ret=sub(ret,1,#ret-2)
  for k=1,w-len do
   ret=ret.." "
  end
  ret=ret.."‚ùé"
 end
 return ret
end
function strspl(s,sep)
 local ret, bffr = {},""
 for i=1, #s do
  if (sub(s,i,i-1+#sep)==sep)then
   add(ret,bffr)
   bffr=""
   i+=#sep-1
  else
   bffr = bffr..sub(s,i,i)
  end
 end
 if (bffr!="") add(ret,bffr)
 return ret
end
