function setwiring(num)
 current_wiring=num
 local w=wirings[num]
 dialog=w.dialog
 minscore=w.minscore
 wires={x=w.x,y=w.y}
 foreach(slots,function(s)
  del(objects,s) end)
 slots=w.slots
 foreach(slots,function(s)
  add(objects,s) end)
 del(objects,cat)
 del(objects,dog)
 if w.cat then add(objects,cat) end
 if w.dog then add(objects,dog) end
 score=getscore()
end

function slot_draw(s)
 local x,y=s.x,s.y
 sh(96+tonum(s.data!=nil),x,y+2)
 if s.data!=nil then
  cubepal(s.data)
  spr(97,x,y+2)
 else
  spr(96,x,y+2)
 end
 pal()
end
function slot_interact(s)
 dialog="looks like a memory location into which a data cube fits."
 if s==leaking and p.data!=nil then
  dialog="this memory location is leaking! you must fix it before placing a data cube."
 elseif s==leaking and p.data==nil then
  dialog="you fix the memory leak.\nlet's hope an animal won't break it again."
  leaking=nil
  sfx(31)
 elseif s.data==nil and p.data!=nil then
  dialog="you place the data cube into the memory location."
  s.data=p.data
  p.data=nil
  sfx(23,-1,4*(s.data-10),4)
 elseif s.data!=nil and p.data==nil then
  dialog="you take the data cube from the memory location."
  p.data=s.data
  s.data=nil
  sfx(22,-1,4*(p.data-10),4)
 elseif s.data!=nil and p.data!=nil then
  dialog="this memory location is full."
 end
 score=getscore()
end
function slot_update(s)
 local pets={cat,dog}
 for i=1,#pets do
  local p=pets[i]
  local adx=abs(s.x-p.x)
  local ady=abs(s.y-p.y)/1.2
  if s.data==nil
   and adx+ady<5
   and leaking!=s then
   leaking=s
   sfx(30)
  end
 end
 if s==leaking and rnd(100)<50 then
  particles.y=s.y+1
  add(particles,{
  x=s.x+4,y=s.y+6,vy=-1-rnd(1),
  life=12,color=rnd(8)+8})
 end
end

function getscore()
 sc=0
 n=#slots-1
 scoring={daniel={},dog={},cat={}}
 for i=1,n do
  --daniel
  if slots[i].data==slots[n-i+1].orig_data then
   sc+=1
   add(scoring.daniel,slots[i])
  end
  --dog
  if wirings[current_wiring].dog
   and i<n
   and slots[i].data!=nil
   and slots[i].data==slots[i].wire
   and slots[i+1].data==slots[i].wire
   then
   sc+=3
   add(scoring.dog,{
    x=(slots[i].x+slots[i+1].x)/2,
    y=(slots[i].y+slots[i+1].y)/2})
  end
  --cat
  if wirings[current_wiring].cat
   and i>1 and i<n
   and slots[i].data!=nil
   and slots[i-1].data!=nil
   and slots[i-1].data==slots[i+1].data
   then
   sc-=1
   add(scoring.cat,slots[i])
  end
 end
 return max(0,sc)
end

function ssh(sx,sy,sw,sh,dx,dy)
 allblack()
 sspr(sx,sy,sw,sh,dx-1,dy)
 sspr(sx,sy,sw,sh,dx+1,dy)
 sspr(sx,sy,sw,sh,dx,dy-1)
 sspr(sx,sy,sw,sh,dx,dy+1)
 pal()
 sspr(sx,sy,sw,sh,dx,dy)
end

function fancynumbers(n,x,y)
 local s=tostr(n).."00"
 if n==0 then s="0" end
 x-=#s*6+(#s-1)\3*3
 for i=1,#s do
  local c=tonum(sub(s,i,i))
  if i>1 and (#s-i)%3==2 then
   ssh(51,56,3,8,x,y+1)
   x+=3
  end
  ssh(c*5,56,5,8,x,y)
  x+=6
 end
end

function cubepal(c)
 if c==10 then
  pal({[7]=10,[6]=4})
 elseif c==11 then
  pal({[7]=11,[6]=3})
 elseif c==12 then
  pal({[7]=12,[6]=1})
 elseif c==14 then
  pal({[7]=8,[6]=2})
 end
end

