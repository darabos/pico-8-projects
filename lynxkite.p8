function draw_metaverse()
 --cyber background
 cls(1)
 local tt=t()/4
 local s=sin(tt/3)*30+160
 local vx=s*sin(tt/8)/2
 for i=1,100 do
  local p=(i+tt)*0.01
  line(
   s*sin(p*3)+vx+64,0,
   s*cos(p*7)+vx+64,96,
   i%3+5)
  line(
   s*sin(p*3)+vx+65,0,
   s*cos(p*7)+vx+65,96,
   i%3+5)
 end
 pal()
 --whiteboard
 local c,w=7,5
 rectfill(10,10,117,85,c)
 for x=1,w do
  line(10,10,10-x,85,c)
  line(117,10,117+x,85,c)
 end
 line(11,9,116,9,0)
 line(10,10,9-w,85,0)
 line(117,10,118+w,85,0)
 line(10-w,86,117+w,86,0)
 spr(28,20,87)
 spr(28,99,87)
 --draw wires
 local wx,wy=wires.x,wires.y
 for i=0,15 do
  for j=0,7 do
   local c=sget(wx+i,wy+j)
   local s
   if sget(wx+i-1,wy+j)>0 then
    s=83
    if sget(wx+i,wy+j-1)>0 then
     s=84
    elseif sget(wx+i,wy+j+1)>0 then
     s=68
    end
   elseif sget(wx+i+1,wy+j)>0 then
    s=83
    if sget(wx+i,wy+j-1)>0 then
     s=82
    elseif sget(wx+i,wy+j+1)>0 then
     s=66
    end
   else
    s=67
   end
   if c==10 then
    pal({[7]=9,[6]=4})
    spr(s,i*8,j*8+16)
   elseif c==11 then
    pal({[7]=11,[6]=3})
    spr(s,i*8,j*8+16)
   elseif c==12 then
    pal({[7]=12,[6]=1})
    spr(s,i*8,j*8+16)
   elseif c==14 then
    pal({[7]=8,[6]=2})
    spr(s,i*8,j*8+16)
   end
  end
 end
 pal()
end
function draw_particles()
